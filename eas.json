{
  "cli": {
    "version": ">= 7.0.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENABLE_BLUETOOTH": "true",
        "EXPO_PUBLIC_ENV": "development"
      },
      "cache": {
        "key": "development-{{ platform }}-{{ hash }}"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENABLE_BLUETOOTH": "true",
        "EXPO_PUBLIC_ENV": "preview"
      },
      "cache": {
        "key": "preview-{{ platform }}-{{ hash }}"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENABLE_BLUETOOTH": "true",
        "EXPO_PUBLIC_ENV": "production"
      },
      "cache": {
        "key": "production-{{ platform }}-{{ hash }}"
      }
    },
    "standalone": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENABLE_BLUETOOTH": "true",
        "EXPO_PUBLIC_ENV": "standalone"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
